{
    "testEnvironment": "jest-bench/environment",
    "reporters": [
        "default",
        "jest-bench/reporter"
    ],
    "testRegex": "(/__benchmarks__/.*|\\.bench)\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["js", "ts", "tsx"],
    "transform": {
        "^.+\\.(ts|tsx)$": "ts-jest"
    }
}
