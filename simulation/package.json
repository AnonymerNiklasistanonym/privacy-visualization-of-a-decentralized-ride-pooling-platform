{
  "name": "simulation",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all compile dist",
    "check:circular": "npx madge --circular --extensions ts src",
    "check:dependencies_unused": "depcruise src",
    "check:exports": "ts-unused-exports tsconfig.json --ignoreFiles=.*/globals/.*",
    "check:load": "loadtest -c 10 --rps 1000 http://localhost:3020/apiv1/participant_coordinates",
    "check": "npm-run-all check:circular check:dependencies_unused",
    "clean": "gts clean",
    "compile": "tsc",
    "dev:test": "jest --watch",
    "dev": "cross-env NODE_ENV=development nodemon --ignore 'dist' --ignore 'build' --ignore 'cache' --watch 'src' --watch 'dist.ts' --watch 'package.json' -e ts,js,hbs,css --exec \"npm-run-all build start\"",
    "dist": "tsx dist.ts",
    "docs": "typedoc --tsconfig tsconfig.json --options typedoc.js",
    "fix": "gts fix",
    "lint": "gts lint",
    "start": "node .",
    "test": "jest"
  },
  "author": "Niklas Mikeler <niklas.mikeler@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.17.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-handlebars": "^7.1.3",
    "h3-js": "^4.1.0",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^16.3.6",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-node": "^11.1.0",
    "gts": "^5.3.1",
    "jest": "^29.7.0",
    "loadtest": "^8.0.9",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.2.2",
    "ts-json-schema-generator": "^2.3.0",
    "ts-unused-exports": "^10.1.0",
    "tsx": "^4.16.2",
    "typedoc": "^0.25.13",
    "typedoc-umlclass": "^0.9.0",
    "typescript": "~5.4.5",
    "typescript-json-schema": "^0.64.0"
  }
}
