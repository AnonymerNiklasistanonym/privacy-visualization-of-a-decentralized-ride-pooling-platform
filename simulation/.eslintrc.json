{
  "extends": [
    "./node_modules/gts/",
    "plugin:jest/recommended"
  ],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "sort-keys": [
      "error",
      "asc",
      {
        "allowLineSeparatedGroups": true,
        "caseSensitive": true,
        "natural": true,
        "minKeys": 2
      }
    ],
    "sort-imports": [
      "error",
      {
        "allowSeparatedGroups": true
      }
    ],
    "no-console": "error"
  }
}
